{
  "name": "mongodb_replica_set",
  "version": "0.1.0",
  "description": "Create a local mongo replica set for dev or testing",
  "main": "index.js",
  "license": "MIT",
  "repository": "https://github.com/stephanebachelier/mongodb_replica_set.git",
  "author": "stephane.bachelier@gmail.com",
  "engines": {
    "node": ">= 8.12.0",
    "npm": ">= 6.4.1"
  },
  "publishConfig": {
    "tag": "latest",
    "private": false,
    "access": "public"
  },
  "files": [
    "index.js",
    "lib",
    "package.json",
    "README.md",
    "CHANGELOG.md"
  ],
  "bin": {
    "mrepset": "bin/cli"
  },
  "scripts": {
    "lint": "standard src/* test/* | snazzy",
    "test:unit": "jest --config .jest.unit.js",
    "test:unit:ci": "jest --config .jest.unit.ci.js --no-color",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release": "release-it"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "**/node_modules/**"
    ],
    "global": [
      "jest",
      "afterAll",
      "afterEach",
      "beforeAll",
      "beforeEach",
      "describe",
      "test",
      "expect"
    ]
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "debug": "^4.1.1",
    "execa": "^1.0.0",
    "fs-extra": "^7.0.1",
    "lodash": "^4.17.11",
    "meow": "^5.0.0",
    "mongodb": "^3.1.13",
    "readline": "^1.3.0",
    "sleepjs": "^3.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "babel-eslint": "^10.0.1",
    "conventional-changelog-cli": "^2.0.12",
    "husky": "^1.3.1",
    "jest": "^24.4.0",
    "jest-chain": "^1.1.2",
    "jest-extended": "^0.11.1",
    "jsonlint": "^1.6.3",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "release-it": "^10.3.1",
    "snazzy": "^8.0.0",
    "standard": "^12.0.1",
    "yaml-lint": "^1.2.4"
  }
}
